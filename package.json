    "typedoc": "^0.28.3"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {},
  "scripts": {
    "definitions": "tsc --project tsconfig.json",
    "prepublishOnly": "npm test && npm run build && npm run check && npm run definitions",
